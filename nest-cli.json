        "tsConfigPath": "apps/chat/tsconfig.app.json"
      }
    },
    "mails": {
      "type": "application",
      "root": "apps/mails",
      "entryFile": "main",
      "sourceRoot": "apps/mails/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mails/tsconfig.app.json"
      }
    },
    "planes-pagos": {
      "type": "application",
      "root": "apps/planes-pagos",
      "entryFile": "main",
      "sourceRoot": "apps/planes-pagos/src",
      "compilerOptions": {
        "tsConfigPath": "apps/planes-pagos/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
